{
  "name": "Light Shades",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Now you can see the haters.",
  "background": {
    "persistent": false,
    "scripts": ["extension.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["box.css"],
      "js": ["js/jquery-2.1.1.js","js/jquery.flot.js", "js/jquery.flot.time.js","js/box.js","extension.js"]
    }
  ],
  "permissions": ["http://*.google.com/", "https://*.google.com/", "https://suhasarehalli.me/*"]
}
